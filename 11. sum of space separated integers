#include <iostream>
using namespace std;

int main()
{
    
    int count = 0,sum=0;
 
    int a[1000000];
 
    
    do {
 

        cin>>a[count++];

    } while (getchar() != '\n' && count < 100);
 
    a[count];
 
    for (int i = 0; i < count; i++) 
    {
        sum+=a[i];
    }
    cout<<sum;
 
    return 0;
}
