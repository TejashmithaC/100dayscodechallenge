#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() 
{
    int n,q,k,i,j,len;
    
    cin>>n>>q;
    
    vector<vector<int>>a(n) ;

    for(i=0;i<n;i++)
    {
        cin>>len;
        a[i].resize(len);
        
        for(j=0;j<len;j++)
        {
            cin>>a[i][j];
        }
    }
    
    for(k=0;k<q;k++)
    {
        cin>>i>>j;
        
        cout<<a[i][j]<<endl;
    }
        
    
       
    return 0;
}
